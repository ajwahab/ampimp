<pinmux-xml schema-version="1" name="ADuCM3029-pinmux.xml"> 

<peripheral name="SPI0" description="SPI0 Module" >
<signal name="SCLK"   port="P0" bit="0" mux="1" pin="0" description="Serial Clock" />
<signal name="MOSI"   port="P0" bit="2" mux="1" pin="1" description="MOSI" />
<signal name="MISO"   port="P0" bit="4" mux="1" pin="2" description="MISO" />
<signal name="CS_0"   port="P0" bit="6" mux="1" pin="3" description="Chip Select-0" />
<signal name="CS_1"   port="P1" bit="20" mux="1" pin="10" description="Chip Select-1" />
<signal name="CS_2"   port="P2" bit="16" mux="2" pin="8" description="Chip Select-2" />
<signal name="CS_3"   port="P2" bit="18" mux="2" pin="9" description="Chip Select-3" />
<signal name="RDY"    port="P1" bit="28" mux="2" pin="14" description="SPI0 RDY Signal" />
</peripheral>

<peripheral name="SPI1" description="SPI1 Module" >
<signal name="SCLK"  port="P1" bit="12" mux="1" pin="6" description="Serial Clock" />
<signal name="MISO"  port="P1" bit="14" mux="1" pin="8" description="MOSI" />
<signal name="MOSI"  port="P1" bit="16" mux="1" pin="7" description="MISO" />
<signal name="CS_0"  port="P1" bit="18" mux="1" pin="9" description="Chip Select-0" />
<signal name="CS_1"  port="P2" bit="22" mux="1" pin="11" description="Chip Select-1" />
<signal name="CS_2"  port="P2" bit="4"  mux="2" pin="2" description="Chip Select-2" />
<signal name="CS_3"  port="P1" bit="20" mux="3" pin="10" description="Chip Select-3" />
<signal name="RDY"   port="P0" bit="28" mux="2" pin="14" description="SPI1 RDY Signal" />
</peripheral>

<peripheral name="SPI2" description="SPI2 Module" >
<signal name="SCLK" port="P1" bit="4"  mux="1" pin="2" description="Serial Clock" />
<signal name="MISO" port="P1" bit="6"  mux="1" pin="3" description="MOSI" />
<signal name="MOSI" port="P1" bit="8"  mux="1" pin="4" description="MISO" />
<signal name="CS_0" port="P1" bit="10" mux="1" pin="5" description="Chip Select-0" />
<signal name="CS_1" port="P0" bit="18" mux="2" pin="9" description="Chip Select-1" />
<signal name="CS_2" port="P2" bit="20" mux="2" pin="10" description="Chip Select-2" />
<signal name="CS_3" port="P2" bit="14" mux="2" pin="7" description="Chip Select-3" />
<signal name="RDY"  port="P0" bit="6"  mux="3" pin="3" description="SPI2 RDY Signal" />
</peripheral>


<peripheral name="I2C0" description="I2C0 Module" >
<signal name="SCL0" port="P0" bit="8"  mux="1" pin="4" description="SCL Line" />
<signal name="SDA0" port="P0" bit="10" mux="1" pin="5" description="SDA Line" />
</peripheral>

<peripheral name="GPIO" description="Port0 and Port1 GPIO" >
<signal name="P0_6" port="P0" bit="12" mux="1" pin="6" description="GPIO Port0 pin 6" />
<signal name="P0_7" port="P0" bit="14" mux="1" pin="7" description="GPIO Port0 pin 7" />
<signal name="P1_1" port="P1" bit="2" mux="1" pin="1" description="GPIO Port1 pin 1" />
</peripheral>

<peripheral name="BEEPER" description="Beeper Module" >
<signal name="TONE_N" port="P0" bit="16"  mux="1" pin="9" description="Tone N" />
<signal name="TONE_P" port="P0" bit="18"  mux="1" pin="8" description="Tone P" />
</peripheral>

<peripheral name="UART0" description="UART0 Module" >
<signal name="Tx" port="P0" bit="20" mux="1" pin="10" description="Tx Data Line" />
<signal name="Rx" port="P0" bit="22" mux="1" pin="11" description="Rx Data Line" />
<signal name="UART_SOUT_EN" port="P0" bit="24" mux="3" pin="12" description="UART_SOUT_EN" />
</peripheral>

<peripheral name="SPORT0_A" description="SPORT0_A Module" >
<signal name="SPT0A_D0"    port="P0" bit="24" mux="1" pin="12" description="SPORT0_A Data" />
<signal name="SPT0A_CLK"   port="P1" bit="30" mux="1" pin="15" description="SPORT0_A CLK" />
<signal name="SPT0A_FS"    port="P2" bit="0"  mux="1" pin="0" description="SPORT0_A FS" />
<signal name="SPT0A_CNV"   port="P2" bit="4"  mux="1" pin="2" description="SPORT0_A CNV" />
</peripheral>

<peripheral name="SPORT0_B" description="SPORT0_B Module" >
<signal name="SPT0B_D0"    port="P0" bit="4" mux="2" pin="2" description="SPORT0_B Data" />
<signal name="SPT0B_CLK"   port="P0" bit="0" mux="2" pin="0" description="SPORT0_B CLK" />
<signal name="SPT0B_FS"    port="P0" bit="2" mux="2" pin="1" description="SPORT0_B FS" />
<signal name="SPT0B_CNV"   port="P0" bit="6" mux="2" pin="3" description="SPORT0_B CNV" />
</peripheral>

<peripheral name="SYS_WAKEUP" description="SYstem Wake up signals" >
<signal name="sys_wake0" port="P0" bit="30" mux="1" pin="15" description="System Wake up 0 signal Line" />
<signal name="sys_wake1" port="P1" bit="0" mux="1" pin="0" description="System Wake up 1 signal Line" />
<signal name="sys_wake2" port="P0" bit="26" mux="1" pin="13" description="System Wake up 2 signal Line" />
<signal name="sys_wake3" port="P2" bit="2" mux="1" pin="1" description="System Wake up 3 signal Line" />
</peripheral>

<peripheral name="RTC" description="RTC1 SensorStrobe" >
<signal name="RTC_SS1" port="P2" bit="22" mux="3" pin="11" description="RTC1 SensorStrobe 1" />
</peripheral>

<peripheral name="TIMER0" description="Timer0 output" >
<signal name="TIMER0_OUT" port="P0" bit="28" mux="1" pin="14" description="Timer0  Out" />
</peripheral>

<peripheral name="TIMER1" description="Timer1 output" >
<signal name="TIMER1_OUT" port="P1" bit="22" mux="2" pin="11" description="Timer1  Out" />
</peripheral>

<peripheral name="TIMER2" description="Timer2 output" >
<signal name="TIMER2_OUT" port="P2" bit="2" mux="2" pin="1" description="Timer2  Out" />
</peripheral>

<peripheral name="SWD0" description=" Serial Wire Debug" >
<signal name="SWD0_CLK" port="P0" bit="12" mux="0" pin="6" description="JTAG or SWD Clock" />
<signal name="SWD0_Data" port="P0" bit="14" mux="0" pin="7" description="JTAG Test Mode Select/Serial Wire Data" />
</peripheral>

<peripheral name="ADC0_IN" description="ADC0 Module" >
<signal name="ADC_CONVERT"    port="P1" bit="26"  mux="1" pin="13"  description="ADC CONVERT" />
<signal name="ADC0_IN0" port="P2" bit="6" mux="1" pin="3" description="ADC0_INPUT-0" />
<signal name="ADC0_IN1" port="P2" bit="8" mux="1" pin="4" description="ADC0_INPUT-1" />
<signal name="ADC0_IN2" port="P2" bit="10" mux="1" pin="5" description="ADC0_INPUT-2" />
<signal name="ADC0_IN3" port="P2" bit="12" mux="1" pin="6" description="ADC0_INPUT-3" />
<signal name="ADC0_IN4" port="P2" bit="14" mux="1" pin="7" description="ADC0_INPUT-4" />
<signal name="ADC0_IN5" port="P2" bit="16" mux="1" pin="8" description="ADC0_INPUT-5" />
<signal name="ADC0_IN6" port="P2" bit="18" mux="1" pin="9" description="ADC0-INPUT-6" />
<signal name="ADC0_IN7" port="P2" bit="20" mux="1" pin="10" description="ADC0_INPUT-7" />
</peripheral>


<peripheral name="SYS_CLK" description="System Clock" >
<signal name="CLOCK_OUT"    port="P2" bit="22"  mux="2" pin="11"  description="System Clock Out" />
<signal name="CLOCK_IN"     port="P1" bit="20"  mux="2" pin="10"  description="System Clock In" />
</peripheral>



